{
    "version": 2,
    "builds": [
        {
            "src": "package.json",
            "use": "@vercel/static-build",
            "config": { "distDir": "dist" }
        }
    ],
    "rewrites": [
        {
            "source": "/Store/api/:path*",
            "destination": "http://52.62.234.97:8080/Store/api/:path*"
        },
        {
            "source": "/uploads/:path*",
            "destination": "http://52.62.234.97:8080/Store/uploads/:path*"
        },
        {
            "source": "/Store/:path*",
            "destination": "http://52.62.234.97:8080/Store/:path*"
        },
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ],
    "headers": [
        {
            "source": "/uploads/:path*",
            "headers": [{ "key": "Cache-Control", "value": "public, max-age=31536000, immutable" }]
        }
    ]
}
